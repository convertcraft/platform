{
  "name": "convertcraft",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "audit:limits": "node scripts/audit-limits.cjs",
    "audit:tools": "node scripts/audit-tool-limits.cjs",
    "audit:bundle": "node scripts/audit-tool-limits-bundle.cjs",
    "audit:bundle:strict": "node scripts/audit-tool-limits-bundle.cjs --strict",
    "check:all": "npm run audit:limits && npm run audit:tools && npm run audit:bundle:strict && npm run build",
    "check:safe": "npm run audit:limits && npm run audit:tools && npm run audit:bundle",
    "postinstall": "node scripts/postinstall.cjs",
    "dev": "node server.js",
    "dev:all": "node scripts/dev-all.mjs",
    "dev:react:watch": "npm --prefix app run build -- --watch",
    "start": "node server.js",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:restart": "pm2 restart ecosystem.config.cjs",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:logs": "pm2 logs",
    "build:react": "npm --prefix app run build",
    "pm2:refresh": "npm run build:react && npm run pm2:restart",
    "build": "node scripts/verify.cjs",
    "gsc:submit": "node scripts/gsc_submit_sitemap.mjs",
    "build:sitemap": "node scripts/build-sitemap.mjs",
    "check:locales": "node scripts/check-locales.mjs",
    "check:panels": "node --check app_deploy/panels.js",
    "lint": "npm run build && npm run check:locales",
    "setup:windows": "powershell -ExecutionPolicy Bypass -File scripts\\setup-dev.ps1",
    "setup:cmd": "scripts\\setup-dev.bat",
    "diagnose": "powershell -ExecutionPolicy Bypass -File scripts\\diagnose.ps1",
    "regen:bundle": "powershell -ExecutionPolicy Bypass -File scripts\\regenerate-deploy-bundle.ps1",
    "stripe:setup": "node scripts/stripe-setup.cjs",
    "stripe:setup:test": "node scripts/stripe-setup.cjs --mode test",
    "stripe:setup:prod": "node scripts/stripe-setup.cjs --mode production",
    "stripe:verify": "node scripts/stripe-verify.cjs",
    "cleanup:auth": "node scripts/cleanup-unverified.cjs",
    "test:e2e": "node scripts/e2e-pro-smoke.mjs",
    "vendor:ai": "node scripts/vendor-ai.cjs",
    "dev:helper": "node api/ffmpeg-helper.js",
    "smoke:endpoints": "node scripts/smoke-endpoints.mjs",
    "smoke:media-helper": "node scripts/smoke-media-helper.mjs",
    "rank:fetch": "node scripts/seo-rank-snapshot.mjs",
    "rank:diff": "node scripts/seo-rank-diff.mjs",
    "rank:report": "node scripts/seo-rank-diff.mjs --latest --dir data/rank-snapshots --out data/rank-snapshots/latest-rank-report.md",
    "rank:weekly": "node scripts/seo-rank-weekly.mjs",
    "ctr:analyze": "node scripts/seo-ctr-analysis.mjs",
    "outreach:dryrun": "node scripts/outreach-sender.mjs --mode dryrun",
    "outreach:send": "node scripts/outreach-sender.mjs --mode send"
  },
  "dependencies": {
    "@ffmpeg/core": "^0.10.0",
    "@ffmpeg/ffmpeg": "^0.10.0",
    "@tensorflow/tfjs": "^4.11.0",
    "@upscalerjs/esrgan-slim": "^1.0.0-beta.12",
    "@upscalerjs/maxim-retouching": "^0.1.0",
    "7z-wasm": "^1.2.0",
    "archiver": "^7.0.1",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "google-auth-library": "^10.5.0",
    "googleapis": "^121.0.0",
    "yargs": "^17.7.2",
    "helmet": "^8.1.0",
    "jpeg-js": "^0.4.4",
    "jsqr": "^1.4.0",
    "jszip": "^3.10.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.4.530",
    "pngjs": "^7.0.0",
    "stripe": "^19.3.1",
    "upscaler": "^1.0.0-beta.19",
    "uuid": "^13.0.0"
  }
}
